{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "composite": false,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "lib": ["ESNext"],
    "baseUrl": "./",
    "types": ["node"],
    "noImplicitAny": true,
    "noEmit": false
  },
  "include": ["vite.config.ts", "env.d.ts", "src/types/env.d.ts"]
}
